{
  "name": "pix-certif",
  "version": "5.203.0",
  "private": false,
  "description": "Plateforme en ligne de gestion des sessions de certification",
  "license": "AGPL-3.0",
  "author": "GIP Pix",
  "engines": {
    "node": "^22.17.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1024pix/pix.git"
  },
  "ember": {
    "edition": "octane"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment $BUILD_ENVIRONMENT",
    "clean": "rm -rf tmp dist node_modules",
    "lint": "run-p --continue-on-error 'lint:!(fix)'",
    "lint:fix": "run-p --continue-on-error lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache --cache-strategy content",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:js:uncached": "eslint .",
    "lint:scss": "stylelint app/styles/*.scss app/styles/**/*.scss",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "lint:translations": "eslint --format node_modules/eslint-plugin-i18n-json/formatter.js translations",
    "lint:translations:fix": "npm run lint:translations -- --fix",
    "preinstall": "npx check-engine",
    "dev": "ember serve --proxy http://localhost:3000",
    "start": "npm run dev",
    "test": "ember test",
    "test:ci": "npx ember exam --split=$CIRCLE_NODE_TOTAL --partition=$((1 + CIRCLE_NODE_INDEX))",
    "test:lint": "npm test && npm run lint",
    "test:watch": "ember exam --serve --reporter dot"
  },
  "overrides": {
    "ember-eslint-parser": "$ember-eslint-parser",
    "ember-source": "$ember-source",
    "@ember/render-modifiers": {
      "ember-source": "$ember-source"
    }
  },
  "devDependencies": {
    "@1024pix/ember-api-actions": "^1.1.0",
    "@1024pix/ember-testing-library": "^3.0.13",
    "@1024pix/eslint-plugin": "^2.1.11",
    "@1024pix/pix-ui": "^55.26.12",
    "@1024pix/stylelint-config": "^5.1.37",
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@ember-data/model": "^5.3.9",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.0",
    "@ember/test-helpers": "^4.0.0",
    "@embroider/compat": "^3.7.1",
    "@embroider/core": "^3.4.20",
    "@embroider/macros": "^1.16.10",
    "@embroider/webpack": "^4.0.9",
    "@formatjs/intl": "^3.0.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "dayjs": "^1.11.6",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "^6.0.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clipboard": "^1.1.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-deprecation-workflow": "^3.0.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-matomo-tag-manager": "^1.3.1",
    "ember-cli-mirage": "^3.0.3",
    "ember-cli-sass": "^11.0.1",
    "ember-composable-helpers": "^5.0.0",
    "ember-cookies": "^1.3.0",
    "ember-data": "^5.3.9",
    "ember-dayjs": "^0.12.4",
    "ember-eslint-parser": "^0.5.7",
    "ember-exam": "9.1.0",
    "ember-file-upload": "^9.3.0",
    "ember-flatpickr": "^8.0.1",
    "ember-inputmask5": "^4.0.2",
    "ember-intl": "^7.1.0",
    "ember-keyboard": "^9.0.1",
    "ember-lifeline": "^7.0.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.0",
    "ember-page-title": "^9.0.0",
    "ember-qunit": "^9.0.0",
    "ember-resolver": "^13.0.0",
    "ember-simple-auth": "^8.0.0",
    "ember-source": "^6.0.0",
    "ember-template-imports": "^4.2.0",
    "ember-template-lint": "^7.0.0",
    "ember-template-lint-plugin-prettier": "^5.0.0",
    "ember-truth-helpers": "^4.0.3",
    "eslint": "^9.26.0",
    "eslint-plugin-chai-expect": "^3.1.0",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-i18n-json": "^4.0.1",
    "eslint-plugin-import-x": "^4.11.1",
    "eslint-plugin-knex": "v0.2.2",
    "eslint-plugin-mocha": "^11.0.0",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-qunit": "^8.1.2",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.1.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.21",
    "miragejs": "^0.1.48",
    "p-queue": "^8.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-ember-template-tag": "^2.0.5",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "sass": "^1.89.0",
    "sinon": "^21.0.0",
    "stylelint": "^16.19.1",
    "tracked-built-ins": "^4.0.0",
    "webpack": "^5.99.8"
  }
}
