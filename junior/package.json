{
  "name": "junior",
  "version": "5.168.0",
  "private": false,
  "description": "Version de Pix à destination des élèves du premier degré",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1024pix/pix.git"
  },
  "license": "AGPL-3.0",
  "author": "GIP Pix",
  "engines": {
    "node": "^22.17.0"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [],
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "run-p --continue-on-error 'lint:!(fix)'",
    "lint:fix": "run-p --continue-on-error lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache --cache-strategy content",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:scss": "stylelint app/styles/*.scss 'app/styles/**/*.scss' 'app/**/*.scss'",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "lint:translations": "eslint --format node_modules/eslint-plugin-i18n-json/formatter.js translations",
    "lint:translations:fix": "npm run lint:translations -- --fix",
    "preinstall": "npx check-engine",
    "dev": "ember serve --proxy http://localhost:3000",
    "start": "npm run dev",
    "test": "ember test",
    "test:ci": "npx ember exam --split=$CIRCLE_NODE_TOTAL --partition=$((1 + CIRCLE_NODE_INDEX))",
    "test:lint": "run-p --continue-on-error test lint"
  },
  "overrides": {
    "ember-data": {
      "@ember/test-helpers": "^5.0.0"
    },
    "ember-intl": {
      "@ember/test-helpers": "^5.0.0"
    }
  },
  "devDependencies": {
    "@1024pix/ember-testing-library": "^3.0.6",
    "@1024pix/epreuves-components": "^0.7.0",
    "@1024pix/eslint-plugin": "^2.1.7",
    "@1024pix/pix-ui": "^55.25.0",
    "@1024pix/stylelint-config": "^5.1.34",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@ember/optional-features": "^2.2.0",
    "@ember/render-modifiers": "^3.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^5.0.0",
    "@embroider/compat": "^3.8.0",
    "@embroider/core": "^3.5.0",
    "@embroider/macros": "^1.16.10",
    "@embroider/webpack": "^4.0.9",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "babel-plugin-ember-template-compilation": "^3.0.0",
    "dotenv": "^17.0.0",
    "ember-auto-import": "^2.7.2",
    "ember-cli": "^6.0.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "^3.0.4",
    "ember-cli-sass": "^11.0.1",
    "ember-data": "~5.3.3",
    "ember-eslint-parser": "^0.5.8",
    "ember-exam": "9.1.0",
    "ember-fetch": "^8.1.2",
    "ember-intl": "^7.1.1",
    "ember-load-initializers": "^3.0.0",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^9.0.0",
    "ember-qunit": "^9.0.0",
    "ember-resolver": "^13.0.0",
    "ember-source": "~6.5.0",
    "ember-template-imports": "^4.2.0",
    "ember-template-lint": "^7.0.0",
    "ember-template-lint-plugin-prettier": "^5.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-ember": "^12.3.3",
    "eslint-plugin-i18n-json": "^4.0.1",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-qunit": "^8.1.2",
    "globals": "^16.0.0",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.21",
    "npm-run-all2": "^8.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-ember-template-tag": "^2.0.2",
    "qunit": "^2.24.0",
    "qunit-dom": "^3.4.0",
    "sass": "^1.83.4",
    "showdown": "^2.1.0",
    "sinon": "^21.0.0",
    "stylelint": "^16.13.2",
    "stylelint-order": "^7.0.0",
    "tracked-built-ins": "^4.0.0",
    "webpack": "5.100.2",
    "xss": "^1.0.14"
  },
  "ember": {
    "edition": "octane"
  }
}
