{
  "name": "pix-admin",
  "version": "5.204.0",
  "private": false,
  "description": "Interface d'administration pour les membres de Pix.",
  "license": "AGPL-3.0",
  "author": "GIP Pix",
  "engines": {
    "node": "^22.17.1"
  },
  "ember": {
    "edition": "octane"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1024pix/pix.git"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment $BUILD_ENVIRONMENT",
    "clean": "rm -rf tmp dist node_modules",
    "lint": "run-p --continue-on-error 'lint:!(fix)'",
    "lint:fix": "run-p --continue-on-error lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache --cache-strategy content",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:js:uncached": "eslint .",
    "lint:scss": "stylelint app/styles/*.scss 'app/styles/**/*.scss'",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "lint:translations": "eslint --format node_modules/eslint-plugin-i18n-json/formatter.js translations",
    "lint:translations:fix": "npm run lint:translations -- --fix",
    "preinstall": "npx check-engine",
    "dev": "ember serve --proxy http://localhost:3000",
    "start": "npm run dev",
    "test": "ember test",
    "test:ci": "npx ember exam --split=$CIRCLE_NODE_TOTAL --partition=$((1 + CIRCLE_NODE_INDEX))",
    "test:lint": "npm test && npm run lint",
    "test:watch": "ember exam --serve --reporter dot"
  },
  "devDependencies": {
    "@1024pix/ember-testing-library": "^3.0.13",
    "@1024pix/eslint-config": "^1.3.8",
    "@1024pix/pix-ui": "^55.26.12",
    "@1024pix/stylelint-config": "^5.1.37",
    "@babel/eslint-parser": "^7.28.0",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.2.2",
    "@embroider/compat": "^3.9.1",
    "@embroider/core": "^3.5.7",
    "@embroider/webpack": "^4.1.1",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "dayjs": "^1.11.13",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "^6.5.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clipboard": "^1.3.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "^3.0.4",
    "ember-cli-sass": "^11.0.1",
    "ember-cookies": "^1.3.0",
    "ember-cp-validations": "^7.0.0",
    "ember-data": "^5.6.0",
    "ember-dayjs": "^0.12.4",
    "ember-eslint-parser": "^0.5.9",
    "ember-exam": "^9.1.0",
    "ember-intl": "^7.3.1",
    "ember-lifeline": "^7.0.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-simple-auth": "^8.0.0",
    "ember-source": "^6.5.0",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^7.9.1",
    "ember-template-lint-plugin-prettier": "^5.0.0",
    "ember-truth-helpers": "^4.0.3",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-ember": "^12.6.0",
    "eslint-plugin-i18n-json": "^4.0.1",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-qunit": "^8.2.4",
    "globals": "^16.3.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "joi": "^17.13.3",
    "js-base64": "^3.7.7",
    "jwt-decode": "^4.0.0",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.21",
    "npm-run-all2": "^8.0.4",
    "p-queue": "^8.1.0",
    "pako": "^2.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-ember-template-tag": "^2.0.6",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "sass": "^1.89.2",
    "showdown": "^2.1.0",
    "sinon": "^21.0.0",
    "stylelint": "^16.21.1",
    "tracked-built-ins": "^4.0.0",
    "webpack": "^5.100.2",
    "xss": "^1.0.15"
  },
  "overrides": {
    "eslint-plugin-ember": {
      "ember-eslint-parser": "$ember-eslint-parser"
    },
    "ember-dayjs": {
      "ember-source": "$ember-source"
    }
  }
}
