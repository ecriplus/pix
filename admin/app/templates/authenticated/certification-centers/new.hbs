{{page-title "Nouveau Centre de certif"}}
<header class="page-header">
  <CertificationCenters::Breadcrumb @currentPageLabel="Nouveau centre de certification" />
</header>

<main class="page-body">
  <section class="page-section">
    <CertificationCenters::CreationForm
      @certificationCenter={{@model.certificationCenter}}
      @habilitations={{@model.habilitations}}
      @onSubmit={{this.addCertificationCenter}}
      @onCancel={{this.goBackToCertificationCentersList}}
    />
  </section>
</main>