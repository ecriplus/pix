{
  "name": "mon-pix",
  "version": "5.169.0",
  "private": false,
  "description": "Plateforme d'évaluation et de certification des compétences numériques",
  "license": "AGPL-3.0",
  "author": "GIP Pix",
  "engines": {
    "node": "^22.17.0"
  },
  "ember": {
    "edition": "octane"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1024pix/pix.git"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "files": [],
  "scripts": {
    "clean": "rm -rf tmp dist node_modules",
    "lint": "run-p --continue-on-error 'lint:!(fix)'",
    "lint:fix": "run-p --continue-on-error 'lint:*:fix'",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache --cache-strategy content",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:js:uncached": "eslint .",
    "lint:scss": "stylelint 'app/**/*.scss'",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "lint:translations": "eslint --format node_modules/eslint-plugin-i18n-json/formatter.js translations",
    "lint:translations:fix": "npm run lint:translations -- --fix",
    "build": "ember build --environment $BUILD_ENVIRONMENT",
    "preinstall": "npx check-engine",
    "dev": "ember serve --proxy http://localhost:3000",
    "start": "ember serve --proxy http://localhost:3000 --live-reload false",
    "test": "ember test",
    "test:ci": "npx ember exam --path=./dist --split=$CIRCLE_NODE_TOTAL --partition=$((1 + CIRCLE_NODE_INDEX))",
    "test:lint": "npm test && npm run lint",
    "test:watch": "ember exam --serve --reporter dot"
  },
  "devDependencies": {
    "@1024pix/ember-api-actions": "^1.1.0",
    "@1024pix/ember-testing-library": "^3.0.6",
    "@1024pix/epreuves-components": "^0.7.0",
    "@1024pix/eslint-plugin": "^2.1.7",
    "@1024pix/pix-ui": "^55.25.0",
    "@1024pix/stylelint-config": "^5.1.34",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@ember-data/json-api": "^5.3.8",
    "@ember-data/model": "^5.3.8",
    "@ember/optional-features": "^2.1.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^5.2.1",
    "@embroider/compat": "^3.0.0",
    "@embroider/core": "^3.0.0",
    "@embroider/macros": "^1.13.5",
    "@embroider/util": "^1.11.1",
    "@embroider/webpack": "^4.0.0",
    "@formatjs/intl": "^3.0.0",
    "@glimmer/component": "^1.1.2",
    "@sentry/ember": "^9.0.0",
    "@testing-library/user-event": "^14.5.2",
    "buffer": "^6.0.3",
    "dayjs": "^1.11.6",
    "dotenv": "^17.0.0",
    "ember-auto-import": "^2.8.1",
    "ember-cli": "^6.0.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-autoprefixer": "^2.0.0",
    "ember-cli-babel": "^8.0.0",
    "ember-cli-clipboard": "^1.0.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-deprecation-workflow": "^3.1.0",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "^3.0.3",
    "ember-cli-sass": "^11.0.1",
    "ember-cookies": "^1.0.0",
    "ember-data": "^5.3.8",
    "ember-dayjs": "^0.12.0",
    "ember-element-helper": "^0.8.6",
    "ember-eslint-parser": "^0.5.0",
    "ember-exam": "^9.0.0",
    "ember-intl": "^7.0.0",
    "ember-keyboard": "^9.0.0",
    "ember-lifeline": "^7.0.0",
    "ember-load-initializers": "^3.0.0",
    "ember-metrics": "^2.0.0",
    "ember-modifier": "^4.0.0",
    "ember-page-title": "^9.0.0",
    "ember-qunit": "^9.0.0",
    "ember-resolver": "^13.0.0",
    "ember-responsive": "^5.0.0",
    "ember-simple-auth": "^8.0.0",
    "ember-source": "^6.3.0",
    "ember-template-imports": "^4.1.1",
    "ember-template-lint": "^7.0.0",
    "ember-template-lint-plugin-prettier": "^5.0.0",
    "ember-test-selectors": "^7.0.0",
    "ember-truth-helpers": "^4.0.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-ember": "^12.2.1",
    "eslint-plugin-i18n-json": "^4.0.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-qunit": "^8.0.0",
    "globals": "^16.0.0",
    "js-yaml": "^4.0.0",
    "jwt-decode": "^4.0.0",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.21",
    "miragejs": "^0.1.45",
    "npm-run-all2": "^8.0.0",
    "p-queue": "^8.0.0",
    "plyr": "^3.7.8",
    "prettier": "^3.0.0",
    "prettier-plugin-ember-template-tag": "^2.0.2",
    "qunit": "^2.19.1",
    "qunit-dom": "^3.2.1",
    "sass": "^1.56.0",
    "showdown": "^2.1.0",
    "sinon": "^21.0.0",
    "stylelint": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "tracked-built-ins": "^4.0.0",
    "webpack": "^5.95.0",
    "xss": "^1.0.13"
  },
  "overrides": {
    "ember-eslint-parser": "$ember-eslint-parser",
    "ember-intl": {
      "@ember/test-helpers": "$@ember/test-helpers"
    },
    "ember-data": {
      "@ember/test-helpers": "$@ember/test-helpers"
    },
    "ember-dayjs": {
      "ember-source": "$ember-source"
    },
    "@ember/render-modifiers": {
      "ember-source": "$ember-source"
    }
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
}
